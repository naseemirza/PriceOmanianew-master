<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thinkbiz.solutions.tbs.com">

    <!--package="com.example.tbs.com"-->

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="thinkbiz.solutions.tbs.com.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="thinkbiz.solutions.tbs.com.LoginActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.MainActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.SignupActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.LoginActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.CardDetails"
            android:label=""
            android:screenOrientation="portrait"
            android:parentActivityName="thinkbiz.solutions.tbs.com.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.GotoSIte"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CardDetailsPkg.CardDetails" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.DialogActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CardDetailsPkg.CardDetails" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.SeeAllOptions"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CardDetailsPkg.DialogActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.AutoEditTextActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CategoryItems.ParentActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.MainActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CategoryItems.ChildActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CategoryItems.ParentActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.CategoryItems.Sub_ChildActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CategoryItems.ChildActivity" />
        <activity
            android:name="thinkbiz.solutions.tbs.com.Options.MoreOptionActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CardDetailsPkg.CardDetails" />
        <activity android:name="thinkbiz.solutions.tbs.com.BottomBar.Activity1" />
        <activity android:name="thinkbiz.solutions.tbs.com.BottomBar.Activity2" />
        <activity android:name="thinkbiz.solutions.tbs.com.BottomBar.Activity3" />
        <activity android:name="thinkbiz.solutions.tbs.com.BottomBar.Activity4" />
        <activity android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.ColorActivity" />
        <activity android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.StorageActivity" />
        <activity android:name="thinkbiz.solutions.tbs.com.CardDetailsPkg.NetworkActivity" />
        <activity android:name="thinkbiz.solutions.tbs.com.CategoryItems.GridActivity"
            android:parentActivityName="thinkbiz.solutions.tbs.com.CategoryItems.ChildActivity"></activity>
    </application>
</manifest>